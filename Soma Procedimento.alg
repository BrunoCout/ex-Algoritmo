algoritmo "SomaProcedimento"
var
   N1, N2: Inteiro
Procedimento Soma(A, B: Inteiro)
var
   S: Inteiro
inicio
   S <- A + B
   Escreval("A soma vale", S)
Fimprocedimento
inicio
   Escreva("Digite um valor: ")
   Leia(N1)
   Escreva("Digite um segundo valor: ")
   Leia(N2)
   Soma(N1, N2)
fimalgoritmo
